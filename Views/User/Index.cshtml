
@model List<User>


    <table class="table text-center">
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Password</th>
            <th>Edit</th>
            <th>Delete</th>
            
        </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Id</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>@user.Password</td>
            
            
                <td>
                    <a asp-controller="User" asp-action="Edit" asp-route-id="@user.Id">Изминеть</a>
                </td>
                <td>
                    <a asp-controller="User" asp-action="Delete" asp-route-id="@user.Id">Удалить</a>
                </td>
            
        </tr>
    }
    </table>
<a asp-controller="User" asp-action="Create"><button class="btn btn-success">Create User</button></a>